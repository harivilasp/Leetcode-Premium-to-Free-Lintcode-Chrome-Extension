{
  "manifest_version": 2,

  "name": "Leetcode to Lintcode",
  "description": "Free access to Leetcode Premium through Lintcode",
  "version": "1.6.1",

  "permissions": [
     "activeTab",
     "tabs",
     "contextMenus",
     "storage"
  ],

  "icons": {
    "19": "img/icon.png",
    "128": "img/icon_store.png"
  },

  "browser_action": {
    "default_icon": "img/icon.png"
  },

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "options_ui": {
    "page": "options.html",
    "chrome_style": false,
    "open_in_tab": true
  }
}
